	"paths": {
		"/auth/register": {
			"post": {
				"operationId": "RegisterUser",
				"responses": {
					"200": {
						"description": "Message informing if create was correct",
						"content": {
							"application/json": {
								"schema": {}
							}
						}
					}
				},
				"description": "Endpoint to create new user in database",
				"tags": [
					"AuthController"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"properties": {
									"firstname": {
										"type": "string",
										"description": "new user name (required)"
									},
									"lastname": {
										"type": "string",
										"description": "new user's last name (required)"
									},
									"email": {
										"type": "string",
										"description": "new user email (required)"
									},
									"password": {
										"type": "string",
										"description": "new user password (required)"
									}
								},
								"required": [
									"firstname",
									"lastname",
									"email",
									"password"
								],
								"type": "object"
							}
						}
					}
				}
			}
		},
		"/auth/login": {
			"post": {
				"operationId": "LoginUser",
				"responses": {
					"200": {
						"description": "Message with jwt access token and jwt refresh token",
						"content": {
							"application/json": {
								"schema": {}
							}
						}
					}
				},
				"description": "Endpoint to auth with login a user in database",
				"tags": [
					"AuthController"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"properties": {
									"email": {
										"type": "string",
										"description": "email of the user to log in (required)"
									},
									"password": {
										"type": "string",
										"description": "password of the user to log in (required)"
									}
								},
								"required": [
									"email",
									"password"
								],
								"type": "object"
							}
						}
					}
				}
			}
		},
		"/auth/refresh": {
			"post": {
				"operationId": "RefreshAccessToken",
				"responses": {
					"200": {
						"description": "Message with a new jwt access token",
						"content": {
							"application/json": {
								"schema": {}
							}
						}
					}
				},
				"description": "Endpoint to refresh the token of a previously logged in user",
				"tags": [
					"AuthController"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"description": "Valid Refresh token",
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"type": "string",
								"description": "Valid Refresh token"
							}
						}
					}
				}
			}
		},
		"/users/me": {
			"get": {
				"operationId": "GetMe",
				"responses": {
